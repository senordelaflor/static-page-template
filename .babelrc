{
  plugins: [
    "syntax-dynamic-import",
    [
      "transform-class-properties",
      {
        spec: true
      }
    ],
    "transform-object-rest-spread"
  ],
  presets: [
    "react",
    [
      "env",
      {
        modules: false,
        targets: {
          browsers: "> 1%",
          uglify: true
        },
        useBuiltIns: true
      }
    ]
  ]
}
